{
    "openapi": "3.0.0",
    "tags": [],
    "paths": {
        "/apn_database/config": {
            "get": {
                "tags": [
                    "APN database"
                ],
                "summary": "Returns multiple entries of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200apnMulti"
                    }
                }
            },
            "post": {
                "tags": [
                    "APN database"
                ],
                "summary": "Creates entry in APN database",
                "responses": {
                    "201": {
                        "$ref": "#/components/responses/200apn"
                    },
                    "422": {
                        "$ref": "#/components/responses/422response"
                    }
                },
                "requestBody": {
                    "description": "Parameter section",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "data": {
                                        "$ref": "#/components/schemas/apn_single_options"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "put": {
                "tags": [
                    "APN database"
                ],
                "summary": "Updates multiple entries of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200apnMulti"
                    },
                    "404": {
                        "$ref": "#/components/responses/404response"
                    },
                    "422": {
                        "$ref": "#/components/responses/422response"
                    }
                },
                "requestBody": {
                    "description": "Parameter section",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "data": {
                                        "type": "array",
                                        "description": "Array of section objects",
                                        "items": {
                                            "$ref": "#/components/schemas/apn_options"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "APN database"
                ],
                "summary": "Deletes multiple entries of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200deleted_multi"
                    },
                    "404": {
                        "$ref": "#/components/responses/404response"
                    }
                },
                "requestBody": {
                    "description": "Parameter section",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "data": {
                                        "type": "array",
                                        "description": "Array of section objects",
                                        "items": {
                                            "type": "string",
                                            "description": "Database entry id"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "x-web": {
                "menu": [
                    "Network",
                    "Mobile",
                    "APN Database"
                ],
                "hw_tags": {
                    "mobile": true
                },
                "name": "APN database"
            }
        },
        "/apn_database/config/{id}": {
            "parameters": [
                {
                    "name": "id",
                    "in": "path",
                    "required": true,
                    "schema": {
                        "type": "string"
                    },
                    "description": "APN database entry identificator (ID)"
                }
            ],
            "get": {
                "tags": [
                    "APN database"
                ],
                "summary": "Returns specified entry of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200apn"
                    },
                    "404": {
                        "$ref": "#/components/responses/404response"
                    }
                }
            },
            "put": {
                "tags": [
                    "APN database"
                ],
                "summary": "Updates specified entry of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200apn"
                    },
                    "404": {
                        "$ref": "#/components/responses/404response"
                    },
                    "422": {
                        "$ref": "#/components/responses/422response"
                    }
                },
                "requestBody": {
                    "description": "Parameter section",
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "data": {
                                        "$ref": "#/components/schemas/apn_single_options"
                                    }
                                }
                            }
                        }
                    }
                }
            },
            "delete": {
                "tags": [
                    "APN database"
                ],
                "summary": "Deletes specified entry of APN database",
                "responses": {
                    "200": {
                        "$ref": "#/components/responses/200deleted"
                    },
                    "404": {
                        "$ref": "#/components/responses/404response"
                    }
                }
            },
            "x-web": {
                "menu": [
                    "Network",
                    "Mobile",
                    "APN Database"
                ],
                "hw_tags": {
                    "mobile": true
                },
                "name": "APN database"
            }
        }
    },
    "components": {
        "responses": {
            "200apn": {
                "description": "Returns database entry",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "boolean",
                                    "description": "Operation status"
                                },
                                "data": {
                                    "$ref": "#/components/schemas/apn_options"
                                }
                            }
                        }
                    }
                }
            },
            "200apnMulti": {
                "description": "Returns database entries",
                "content": {
                    "application/json": {
                        "schema": {
                            "type": "object",
                            "properties": {
                                "success": {
                                    "type": "boolean",
                                    "description": "Operation status"
                                },
                                "data": {
                                    "type": "array",
                                    "description": "Array of section objects",
                                    "items": {
                                        "$ref": "#/components/schemas/apn_options"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "schemas": {
            "apn_single_options": {
                "type": "object",
                "properties": {
                    "carrier": {
                        "type": "string",
                        "description": "Name of a company that sells wireless connectivity to customers for cellphone data and telephone calls. It may also be called a mobile network operator, a mobile carrier, cellular company or wireless service provider.",
                        "maxLength": 32,
                        "x-web": {
                            "pretty": "Carrier name",
                            "input-type": "input"
                        }
                    },
                    "mcc": {
                        "type": "string",
                        "description": "A mobile code consisting of three digits used to identify GSM networks. MCC is also used along with the International Mobile Subscriber Identity (IMSI) to identify the region from which mobile subscriber belongs.",
                        "format": "regex",
                        "pattern": "^[0-9]+$",
                        "minLength": 3,
                        "maxLength": 3,
                        "x-web": {
                            "pretty": "Mobile Country Code (MCC)",
                            "input-type": "input"
                        }
                    },
                    "mnc": {
                        "type": "string",
                        "description": "Mobile Network Code (MNC) is a unique two- or three-digit number used to identify a home Public Land Mobile Network (PLMN) to. MNC is allocated by the national regulator.",
                        "format": "regex",
                        "pattern": "^[0-9]+$",
                        "minLength": 1,
                        "maxLength": 3,
                        "x-web": {
                            "pretty": "Mobile Network Code (MNC)",
                            "input-type": "input"
                        }
                    },
                    "apn": {
                        "type": "string",
                        "description": "APN (Access Point Name) is configurable network identifier used by a mobile device when connecting to a carrier",
                        "maxLength": 62,
                        "format": "regex",
                        "pattern": "(^[a-zA-Z0-9]$)|(^([a-zA-Z0-9][a-zA-Z0-9.-]*[a-zA-Z0-9])$)",
                        "x-web": {
                            "pretty": "APN (Access Point Name)",
                            "input-type": "input"
                        }
                    },
                    "user": {
                        "type": "string",
                        "maxLength": 64,
                        "description": "Username to connect to GSM carrier's network.",
                        "x-web": {
                            "pretty": "Username",
                            "input-type": "input"
                        }
                    },
                    "password": {
                        "type": "string",
                        "maxLength": 64,
                        "description": "Password to connect to GSM carrier's network.",
                        "x-web": {
                            "pretty": "Password",
                            "input-type": "input"
                        }
                    },
                    "authtype": {
                        "type": "string",
                        "description": "Authentication method that GSM carrier uses to authenticate new connections on it's network.",
                        "enum": [
                            "0",
                            "1",
                            "2"
                        ],
                        "default": "0",
                        "x-web": {
                            "pretty": "Authentication type",
                            "enum-pretty": {
                                "0": "None",
                                "1": "PAP",
                                "2": "CHAP"
                            },
                            "input-type": "select"
                        }
                    },
                    "pdptype": {
                        "type": "string",
                        "description": "Address type that is requested from the operator.",
                        "enum": [
                            "0",
                            "1",
                            "2"
                        ],
                        "default": "0",
                        "x-web": {
                            "pretty": "PDP Type",
                            "enum-pretty": {
                                "0": "IPv4v6",
                                "1": "IPv4",
                                "2": "IPv6"
                            },
                            "input-type": "select"
                        }
                    },
                    "country": {
                        "type": "string",
                        "readOnly": true,
                        "description": "Operator country name.",
                        "default": "Other",
                        "x-web": {
                            "pretty": "Country name"
                        }
                    }
                },
                "required": [
                    "carrier",
                    "mcc",
                    "mnc",
                    "apn"
                ]
            },
            "apn_options": {
                "allOf": [
                    {
                        "type": "object",
                        "properties": {
                            "id": {
                                "type": "string",
                                "description": "APN database entry identificator (ID)."
                            }
                        },
                        "required": [
                            "id"
                        ]
                    },
                    {
                        "$ref": "#/components/schemas/apn_single_options"
                    }
                ]
            }
        }
    }
}