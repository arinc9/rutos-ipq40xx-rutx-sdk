  "components": {
    "responses": {
      "200tr069": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "$ref": "#/components/schemas/tr069_options_resp"
                }
              },
              "required": ["success", "data"]
            }
          }
        }
      },
      "200tr069Multi": {
        "description": "",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "uniqueItems": true,
                  "items": {
                    "$ref": "#/components/schemas/tr069_options_resp"
                  }
                }
              },
              "required": ["success", "data"]
            }
          }
        }
      }
    },
    "schemas": {
      "tr069_single_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "description": "Enables TR-069 client",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "periodic_enable": {
            "type": "string",
            "description": "Enables TR-069 client periodic data transmission to TR-069 server",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Periodic enable",
              "input-type": "switch"
            }
          },
          "allow_ra": {
            "type": "string",
            "description": "Accept server request",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Accept server request",
              "input-type": "switch"
            }
          },
          "periodic_interval": {
            "type": "string",
            "description": "Periodic data transmission interval (allowed: 60s-9999999s)",
            "format": "irange(60, 9999999)",
            "default": "3600",
            "x-web": {
              "pretty": "Sending interval",
              "input-type": "input"
            }
          },
          "username": {
            "type": "string",
            "description": "User name for authentication on TR-069 server. Allowed characters (a-zA-Z0-9!@#$%&*+-/=?^_`{|}~. )",
            "format": "regex",
            "pattern": "^[a-zA-Z0-9!@#$%&*+/=?^_`{|}~. -]+$",
            "maxLength": 64,
            "x-web": {
              "pretty": "Username",
              "input-type": "input"
            }
          },
          "password": {
            "type": "string",
            "description": "Password for authentication on TR-069 server",
            "format": "credentials_validate",
            "maxLength": 64,
            "writeOnly": true,
            "x-web": {
              "pretty": "Password",
              "input-type": "input"
            }
          },
          "url": {
            "type": "string",
            "description": "TR-069 server's URL to send data to",
            "format": "url",
            "maxLength": 128,
            "default": "http://192.168.1.110:8080/openacs/acs",
            "x-web": {
              "pretty": "URL",
              "input-type": "input"
            }
          }
        }
      },
      "tr069_single_options_resp": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "description": "Enables TR-069 client",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "periodic_enable": {
            "type": "string",
            "description": "Enables TR-069 client periodic data transmission to TR-069 server",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Periodic enable",
              "input-type": "switch"
            }
          },
          "allow_ra": {
            "type": "string",
            "description": "Accept server request",
            "format": "is_bool",
            "enum": ["0", "1"],
            "x-web": {
              "pretty": "Accept server request",
              "input-type": "switch"
            }
          },
          "periodic_interval": {
            "type": "string",
            "description": "Periodic data transmission interval (allowed: 60s-9999999s)",
            "format": "irange(60, 9999999)",
            "default": "3600",
            "x-web": {
              "pretty": "Sending interval",
              "input-type": "input"
            }
          },
          "username": {
            "type": "string",
            "description": "User name for authentication on TR-069 server. Allowed characters (a-zA-Z0-9!@#$%&*+-/=?^_`{|}~. )",
            "format": "regex",
            "pattern": "^[a-zA-Z0-9!@#$%&*+/=?^_`{|}~. -]+$",
            "maxLength": 64,
            "x-web": {
              "pretty": "Username",
              "input-type": "input"
            }
          },
          "password": {
            "type": "string",
            "description": "Indicates if 'password' option is set. 'unset' for not set, 'set' for configured.",
            "readOnly": true,
            "enum": ["unset", "set"]
          },
          "url": {
            "type": "string",
            "description": "TR-069 server's URL to send data to",
            "format": "url",
            "maxLength": 128,
            "default": "http://192.168.1.110:8080/openacs/acs",
            "x-web": {
              "pretty": "URL",
              "input-type": "input"
            }
          }
        }
      },
      "tr069_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Configuration ID"
              }
            }
          },
          {
            "$ref": "#/components/schemas/tr069_single_options"
          }
        ],
        "required": ["id"]
      },
      "tr069_options_resp": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Configuration ID"
              }
            }
          },
          {
            "$ref": "#/components/schemas/tr069_single_options_resp"
          }
        ],
        "required": ["id"]
      }
    }
  }
