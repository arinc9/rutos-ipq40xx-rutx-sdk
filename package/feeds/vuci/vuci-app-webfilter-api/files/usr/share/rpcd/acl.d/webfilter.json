{
  "services/webfilter/site": {
    "description": "Web Filter site blocking",
    "read": {
      "api": [
        "/webfilter/global/*",
        "/webfilter/config/*",
        "/hotspot/status/*",
        "/interfaces/config/*"
      ]
    },
    "write": {
      "api": [
        "/webfilter/global/*",
        "/webfilter/config/*"
      ]
    }
  },
  "services/webfilter/proxy": {
    "description": "proxy based content blocker",
    "read": {
      "api": [
        "/webfilter/privoxy/*"
      ]
    },
    "write": {
      "api": [
        "/webfilter/privoxy/*"
      ]
    }
  }
}
