{
	"services/hotspot/general": {
		"read": {
			"api": [
				"/hotspot/config/*",
				"/hotspot/options/*",
				"/hotspot/status/*",
				"/interfaces/status/*",
				"/certificates/config/*",
				"/system/device/status/*",
				"/hotspot/users/config/*",
				"/hotspot/groups/config/*",
				"/modems/status_basic/*",
				"/wireless/interfaces/status/*",
				"/dhcp/servers/ipv4/config/*",
				"/users/config/*",
				"/wireless/interfaces/config/*"
			]
		},
		"write": {
			"api": [
				"/hotspot/config/*",
				"/hotspot/options/*",
				"/hotspot/status/*"
			]
		}
	},
	"services/hotspot/users": {
		"description": "hotspot users configuration",
