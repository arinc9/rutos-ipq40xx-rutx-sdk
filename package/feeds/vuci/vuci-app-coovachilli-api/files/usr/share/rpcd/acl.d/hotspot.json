	"services/hotspot/groups": {
		"description": "hotspot groups configuration",
		"read": {
			"api": [
				"/hotspot/groups/*"
			]
		},
		"write": {
			"api": [
				"/hotspot/groups/*"
			]
		}
	},
	"services/hotspot/landing": {
		"description": "hotspot landing page configuration",
		"read": {
			"api": [
				"/hotspot/themes/*",
				"/hotspot/images/*"
			]
		},
		"write": {
			"api": [
				"/hotspot/themes/*",
				"/hotspot/images/*"
			]
		}
	},
	"services/hotspot/user_management": {
		"description": "hotspot users management",
		"read": {
			"api": [
				"/hotspot/user_management/*"
			]
		},
		"write": {
			"api": [
				"/hotspot/user_management/*"
			]
		}
