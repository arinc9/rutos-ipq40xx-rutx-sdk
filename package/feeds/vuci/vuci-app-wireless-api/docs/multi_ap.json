  "components": {
    "responses": {
      "access_point_config": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                "type": "boolean",
                "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                    "ssid": {
                      "type": "string",
                      "description": "SSID of the access point."
                    },
                    "id": {
                      "type": "string",
                      "description": "The ID of the Multi AP configuration"
                    },
                    "priority": {
                      "type": "string",
                      "description": "Priority of the AP (lower value means higher priority). The order in which the wifi interface will try to connect to the access points."
                    },
                    "enabled": {
                      "type": "string",
                      "description": "If disabled, wifi interface won't try to connect to this AP."
                    }
                    },
                    "required": [
                      "ssid",
                      "id",
                      "priority",
                      "enabled"
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "multi_ap_access_point_name_index": {
        "allOf": [
          {
            "title": "multi_ap_access_point",
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "The ID of the Multi AP configuration",
                "x-web": {
                  "pretty": "Configuration ID",
                  "input-type": "input"
                }
              }
            }
          },
          {
            "$ref": "#/components/schemas/multi_ap_access_point_index"
          }
        ]
      },
      "multi_ap_access_point_index": {
        "title": "multi_ap_access_point",
        "type": "object",
        "properties": {
          "ssid": {
            "type": "string",
            "description": "SSID of the access point.",
            "x-web": {
              "unique": true,
              "pretty": "SSID",
              "input-type": "input"
            }
          },
          "key": {
            "type": "string",
            "minLength": 8,
            "maxLength": 64,
            "description": "Password of the access point.",
            "x-web": {
              "pretty": "Key",
              "input-type": "input"
            }
          },
          "enabled": {
            "type": "string",
            "enum": [
              "0",
              "1"
            ],
            "description": "If disabled, wifi interface won't try to connect to this AP.",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "priority": {
            "type": "string",
            "description": "Priority of the AP (lower value means higher priority). The order in which the wifi interface will try to connect to the access points.",
            "x-web": {
              "pretty": "Priority",
              "input-type": "input"
            }
          }
        }
      }
    },
    "requestBodies": {
      "access_point_config": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "file": {
                  "type": "string",
                  "format": "binary",
                  "description": "Multi AP file."
                }
              },
              "required": [
                "file"
              ]
            }
          },
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                "$ref": "#/components/schemas/multi_ap_access_point_index"
                }
              }
            }
          }
        },
        "description": "If content-type is multipart/form-data then a file is uploaded with a list of access points. File format example:  \n\nssid: my_ssid\n\nenable: 0\n\nkey: my_password\n\nssid: ssid with no key\n\nssid: another_ssid\n\nkey: another_pass"
      }
    }
  }
