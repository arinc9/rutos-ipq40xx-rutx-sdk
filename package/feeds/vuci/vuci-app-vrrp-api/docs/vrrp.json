  "components": {
    "schemas": {
      "vrrp_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "type": "string",
            "description": "Enable VRRP (Virtual Router Redundancy Protocol) for interface.",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            }
          },
          "virtual_mac": {
            "description": "Allow VRRP to use virtualized MAC for interface.",
            "type": "string",
            "x-web": {
              "pretty": "Virtualize MAC",
              "input-type": "switch"
            }
          },
          "virtual_id": {
            "description": "Routers with same IDs will be grouped in the same VRRP (Virtual Router Redundancy Protocol) cluster, range [1 - 255].",
            "type": "string",
            "x-web": {
              "pretty": "Virtualize ID",
              "input-type": "input"
            }
          },
          "priority": {
            "description": "Router with highest priority value on the same VRRP (Virtual Router Redundancy Protocol) cluster will act as a main one, range [1 - 255].",
            "type": "string",
            "x-web": {
              "pretty": "Priority",
              "input-type": "input"
            }
          },
          "delay": {
            "description": "Time interval in seconds between advertisements, range [1 - 255].",
            "type": "string",
            "x-web": {
              "pretty": "Advertisement interval",
              "input-type": "input"
            }
          },
          "interface": {
            "type": "string",
            "description": "Select which interface VRRP will operate on.",
            "x-web": {
              "pretty": "Interface",
              "input-type": "input"
            }
          },
          "virtual_ip": {
            "description": "Virtual IP address(es) for interface's VRRP (Virtual Router Redundancy Protocol) cluster.",
            "type": "array",
            "items": {
              "type": "string"
            },
            "x-web": {
              "pretty": "IP address",
              "input-type": "input"
            }
          },
          "ping_enabled": {
            "description": "Enable connection checking.",
            "type": "string",
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "host": {
            "type": "string",
            "description": "IP address or hostname to ping, e.g. 192.168.1.1 or www.host.com.",
            "x-web": {
              "pretty": "IP address or hostname",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "interval": {
            "description": "Time interval between two pings (seconds).",
            "type": "string",
            "x-web": {
              "pretty": "Ping interval",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "time_out": {
            "type": "string",
            "description": "Time to receive ping response (seconds).",
            "x-web": {
              "pretty": "Ping timeout",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "packet_size": {
            "type": "string",
            "description": "Ping packet size (bytes).",
            "x-web": {
              "pretty": "Ping packet size",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "ping_attempts": {
            "type": "string",
            "description": "Number of ping packets to send.",
            "x-web": {
              "pretty": "Ping attempts",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          },
          "retry": {
            "type": "string",
            "description": "Number of pings to retry, before switching to backup router mode.",
            "x-web": {
              "pretty": "Ping retry count",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            }
          }
        },
        "x-examples": {}
      },
