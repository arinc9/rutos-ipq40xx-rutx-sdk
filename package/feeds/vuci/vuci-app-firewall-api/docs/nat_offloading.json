{
  "openapi": "3.0.0",
  "tags": [],
  "paths": {
    "/nat_offloading/global": {
      "get": {
        "summary": "Returns Firewall NAT Offloading settings",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200NatOff"
          }
        },
        "tags": [
          "General"
        ]
      },
      "put": {
        "summary": "Updates Firewall NAT Offloading settings",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200NatOff"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "description": "Parameter section",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "$ref": "#/components/schemas/nat_offloading_options"
                  }
                }
              }
            }
          }
        },
        "tags": [
          "General"
        ]
      },
      "x-web": {
        "name": "NAT Offloading",
        "menu": [
          "Network",
          "Firewall",
          "NAT Offloading"
        ],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false,
          "nat_offloading": true
        }
      }
    }
  },
  "components": {
    "responses": {
      "200NatOff": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "description": "Operation status",
                  "type": "boolean"
                },
                "data": {
                  "$ref": "#/components/schemas/nat_offloading_options"
                }
              }
            }
          }
        }
      },
      "200NatOffMulti": {
        "description": "Returns sections",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "description": "Operation status",
                  "type": "boolean"
                },
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/nat_offloading_options"
                  }
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "nat_offloading_options": {
        "type": "object",
        "properties": {
          "flow_offloading": {
            "type": "string",
            "description": "Software based offloading for routing/NAT",
            "x-web": {
              "pretty": "Software flow offloading",
              "input-type": "switch"
            }
          },
          "flow_offloading_hw": {
            "type": "string",
            "description": "Hardware based offloading for routing/NAT",
            "x-web": {
              "pretty": "Hardware flow offloading",
              "input-type": "switch",
              "depends": [
                {
                  "flow_offloading": "1"
                }
              ],
              "hw_tags": {
                "hw_nat": true
              }
            }
          }
        }
      }
    }
  }
}