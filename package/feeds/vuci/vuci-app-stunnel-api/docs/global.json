{
  "openapi": "3.0.0",
  "info": {
    "title": "STunnel general",
    "description": "STunnel general API documentation",
    "version": "1.0",
    "contact": {}
  },
  "servers": [],
  "paths": {
    "/stunnel/global": {
      "get": {
        "summary": "Returns specified stunnel global section",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200stunnel_global"
          }
        },
        "tags": ["General"]
      },
      "put": {
        "summary": "Updates specified stunnel global configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200stunnel_global"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "$ref": "#/components/schemas/stunnel_global_options"
                  }
                }
              },
              "examples": {
                "example-1": {
                  "value": {
                    "data": {
                      "enabled": "1",
                      "debug": "5",
                      "use_alt": "1",
                      "alt_config_file": ""
                    }
                  }
                }
              }
            }
          },
          "description": ""
        },
        "tags": ["General"]
      },
      "parameters": [],
      "x-web": {
        "menu": ["Services", "VPN", "STunnel"],
        "hw_tags": {
          "is_switch": false,
          "is_access_point": false
        }
      }
    }
  },
  "components": {
    "responses": {
      "200stunnel_global": {
        "description": "Returns section",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "$ref": "#/components/schemas/stunnel_global_options"
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "stunnel_global_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enabled",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable service"
          },
          "debug": {
            "x-web": {
              "pretty": "Debug Level",
              "input-type": "input"
            },
            "type": "string",
            "description": "Debug Level (0"
          },
          "use_alt": {
            "x-web": {
              "pretty": "Use alternative config",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Set to use alternative config"
          },
          "alt_config_file": {
            "x-web": {
              "pretty": "Upload alternative config",
              "input-type": "input",
              "depends": [
                {
                  "use_alt": "1"
                }
              ]
            },
            "type": "string",
            "format": "binary",
            "description": "Alternative config file path"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "General"
    }
  ]
}
