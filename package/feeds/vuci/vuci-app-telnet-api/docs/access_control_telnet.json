{
  "openapi": "3.0.0",
  "tags": [
    {
      "name": "Telnet"
    }
  ],
  "paths": {
    "/access_control/telnet/config": {
      "get": {
        "summary": "Returns Telnet configuration",
        "tags": ["Telnet"],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlTelnet"
          }
        }
      },
      "put": {
        "summary": "Updates Telnet configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlTelnet"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/telnet_request"
        },
        "tags": ["Telnet"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        }
      }
    },
    "/access_control/telnet/config/{id}": {
      "parameters": [
        {
          "name": "id",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "Configuration ID"
        }
      ],
      "get": {
        "summary": "Returns Telnet configuration",
        "tags": ["Telnet"],
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlTelnetSingle"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          }
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Configuration ID"
          }
        ]
      },
      "put": {
        "summary": "Updates Telnet configuration",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200AccessControlTelnetSingle"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/telnet_request_single"
        },
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "Configuration ID"
          }
        ],
        "tags": ["Telnet"]
      },
      "x-web": {
        "menu": ["System", "Administration", "Access Control", "General"],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        }
      }
    }
  },
  "components": {
    "responses": {
      "200AccessControlTelnet": {
        "description": "Returns configuration",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/access_control_telnet_options"
                  }
                }
              }
            }
          }
        }
      },
      "200AccessControlTelnetSingle": {
        "description": "Returns configuration",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "$ref": "#/components/schemas/access_control_telnet_options"
                }
              }
            }
          }
        }
      }
    },
    "schemas": {
      "access_control_telnet_options": {
        "allOf": [
          {
            "type": "object",
            "properties": {
              "id": {
                "type": "string",
                "description": "Configuration ID"
              }
            }
          },
          {
            "$ref": "#/components/schemas/access_control_telnet_single_options"
          }
        ]
      },
      "access_control_telnet_single_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": " Enable Telnet access",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Turns Telnet access from the local network (LAN) on or off.",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "wan_access": {
            "x-web": {
              "pretty": "Enable remote Telnet access",
              "input-type": "switch",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            },
            "type": "string",
            "description": "Set to access the router via Telnet from the outside (WAN).",
            "format": "is_bool",
            "enum": ["0", "1"]
          },
          "port": {
            "x-web": {
              "pretty": "Telnet port",
              "input-type": "input",
              "depends": [
                {
                  "enabled": "1"
                }
              ]
            },
            "type": "string",
            "description": "Port to listen for Telnet access.",
            "format": "port"
          }
        },
        "required": ["enabled", "wan_access", "port"]
      }
    },
    "requestBodies": {
      "telnet_request": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "description": "Array of section objects",
                  "items": {
                    "$ref": "#/components/schemas/access_control_telnet_options"
                  }
                }
              },
              "required": ["data"]
            }
          }
        }
      },
      "telnet_request_single": {
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "$ref": "#/components/schemas/access_control_telnet_single_options"
                }
              },
              "required": ["data"]
            }
          }
        }
      }
    }
  }
}
