{
  "openapi": "3.0.0",
  "paths": {
    "/tr069/config": {
      "get": {
        "tags": ["General"],
        "summary": "Returns the TR-069 configuration in an array",
        "description": "",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200tr069Multi"
          }
        }
      },
      "put": {
        "tags": ["General"],
        "summary": "Updates the TR-069 configuration in an array",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200tr069"
          },
          "404": {
            "$ref": "#/components/responses/404response"
          },
          "422": {
            "$ref": "#/components/responses/422response"
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "data": {
                    "type": "array",
                    "uniqueItems": true,
                    "minItems": 1,
                    "items": {
                      "$ref": "#/components/schemas/tr069_options"
                    }
                  }
                },
                "required": ["data"]
              }
            }
          }
        }
      },
      "x-web": {
        "menu": ["Services", "TR-069", "TR-069"],
        "hw_tags": {
