    "schemas": {
      "tailscale_options": {
        "type": "object",
        "properties": {
          "enabled": {
            "x-web": {
              "pretty": "Enable",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Enable Tailscale service",
            "format": "is_bool"
          },
          "auth_key": {
            "x-web": {
              "pretty": "Authentication key",
              "input-type": "input"
            },
            "type": "string",
            "description": "Provide an auth key to automatically authenticate the node as your user account.",
            "format": "regex",
            "pattern": "^[a-zA-Z0-9-]+$",
            "maxLength": 64,
            "minLength": 22
          },
          "advert_routes": {
            "x-web": {
              "pretty": "Advertise routes",
              "input-type": "input"
            },
            "type": "array",
            "description": "Expose physical subnet routes to your entire Tailscale network.",
            "items": {
              "type": "string",
              "format": "cidr4"
            }
          },
          "accept_routes": {
            "x-web": {
              "pretty": "Accept routes",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Accept subnet routes that other nodes advertise.",
            "format": "is_bool"
          },
          "exit_node": {
            "x-web": {
              "pretty": "Exit node",
              "input-type": "switch"
            },
            "type": "string",
            "description": "Offer to be an exit node for outbound internet traffic from the Tailscale network.",
            "format": "is_bool"
          },
          "auth_type": {
            "x-web": {
              "pretty": "Authentication method",
              "input-type": "select",
              "enum-pretty": {
                "key": "Key",
                "url": "URL"
              }
            },
            "type": "string",
            "description": "Authentication method can be 'url' or 'key'",
            "enum": ["key", "url"]
          }
        },
        "required": ["auth_type"]
