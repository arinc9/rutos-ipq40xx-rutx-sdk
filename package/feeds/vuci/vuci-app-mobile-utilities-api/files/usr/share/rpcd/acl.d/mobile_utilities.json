
{
	"services/mobile_utilities/sms_utilities": {
		"description": "mobile utilities configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": [
					"get_all_modems"
				],
				"sim": [
					"get"
				]
			},
			"api": [
				"/sms_utilities/*",
				"/recipients/phone_groups/*",
				"/modems/status*"
			]
		},
		"write": {
			"api": [
				"/sms_utilities/*",
				"/recipients/phone_groups/*"
			]
		}
	},
	"services/mobile_utilities/sms_gateway/post_get": {
		"description": "post/get configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": ["/modems/status*", "/sms_gateway/post_get/*"]
		},
		"write": {
			"api": ["/sms_gateway/post_get/*"]
		}
	},
	"services/mobile_utilities/sms_gateway/auto_reply": {
		"description": "auto reply configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": [
				"/sms_gateway/auto_reply/*",
				"/recipients/phone_groups/*",
				"/modems/status*"
			]
		},
		"write": {
			"api": [
				"/sms_gateway/auto_reply/*"
			]
		}
	},
	"services/mobile_utilities/sms_gateway/sms_forwarding": {
		"description": "sms forwarding configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": [
				"/sms_gateway/sms_forwarding/*",
				"/recipients/phone_groups/*",
				"/recipients/email_users/*",
				"/modems/status*"
			]
		},
		"write": {
			"api": [
				"/sms_gateway/sms_forwarding/*"
			]
		}
	},
	"services/mobile_utilities/sms_gateway/email_to_sms": {
		"description": "email to sms configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": ["/sms_gateway/email_to_sms/*", "/modems/status*"]
		},
		"write": {
			"api": ["/sms_gateway/email_to_sms/*"]
		}
	},
	"services/mobile_utilities/call_utilities": {
		"description": "call utilities configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": [
				"/recipients/phone_groups/*",
				"/call_utilities/*",
				"/modems/status*"
			]
		},
		"write": {
			"api": [
				"/call_utilities/*"
			]
		}
	},
	"services/mobile_utilities/sms_messages/read": {
		"description": "read messages configuration",
		"read": {
			"ubus": {
				"vuci.network.mobile": ["get_all_modems"]
			},
			"api": ["/modems/status*", "/messages/status/*", "/messages/modem/*"]
		},
		"write": {
