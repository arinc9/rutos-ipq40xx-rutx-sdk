{
  "openapi": "3.0.0",
  "tags": [],
  "paths": {
    "/uscripts/config": {
      "get": {
        "tags": ["General"],
        "summary": "Returns startup script file contents",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200UscriptsGet"
          }
        }
      },
      "x-web": {
        "menu": ["System", "Maintenance", "Custom Scripts"],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        },
        "name": "Custom Scripts"
      }
    },
    "/uscripts/actions/upload": {
      "post": {
        "summary": "Uploads a startup script file",
        "responses": {
          "200": {
            "$ref": "#/components/responses/200Uscripts"
          }
        },
        "requestBody": {
          "$ref": "#/components/requestBodies/file_upload_uscripts"
        },
        "tags": ["General"]
      },
      "x-web": {
        "menu": ["System", "Maintenance", "Custom Scripts"],
        "hw_tags": {
          "is_access_point": false,
          "is_switch": false
        },
        "name": "Custom Scripts"
      }
    }
  },
  "components": {
    "responses": {
      "200Uscripts": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "path": {
                      "type": "string",
                      "description": "Path of the uploaded script"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "200UscriptsGet": {
        "description": "Example response",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "success": {
                  "type": "boolean",
                  "description": "Operation status"
                },
                "data": {
                  "type": "object",
                  "properties": {
                    "script": {
                      "x-web": {
                        "pretty": "Startup script",
                        "input-type": "input"
                      },
                      "type": "string",
                      "description": "Contents of the script."
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "requestBodies": {
      "file_upload_uscripts": {
        "content": {
          "multipart/form-data": {
            "schema": {
              "type": "object",
              "properties": {
                "file": {
                  "x-web": {
                    "pretty": "Startup script"
                  },
                  "type": "string",
                  "format": "binary",
                  "description": "File to upload."
                }
              },
              "required": ["file"]
            }
          }
        }
      }
    }
  }
}
