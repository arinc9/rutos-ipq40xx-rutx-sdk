{
	"network/firewall/zones": {
		"description": "firewall zones configuration",
		"read": {
			"api": [
				"/nat_offloading/*",
				"/firewall/*",
				"/interfaces/*",
				"/dmvpn/*",
				"/gre/*",
				"/l2tp/client/*",
				"/l2tpv3/*",
				"/pptp/client/*",
				"/sstp/*",
				"/wireguard/*"
			]
		},
		"write": {
			"api": [
				"/nat_offloading/*",
				"/firewall/*"
			]
		}
	},

	"network/firewall/forwards": {
		"description": "firewall forwards configuration",
		"read": {
